<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:a="urn:import:gwt.material.design.addins.client.sideprofile">
	<a:MaterialSideProfile url="../img/userProfKim.svg">
		<m:html.Div float="RIGHT" >
		<m:MaterialImage ui:field="img" addStyleNames="profileImg" type="CIRCLE"/>
		<m:MaterialLabel ui:field="name" textColor="white" addStyleNames="profileName" />
		<m:MaterialLink ui:field="acctId" textColor="white" activates="ddl" addStyleNames="profileName"/>
		<m:MaterialDropDown activator="ddl">
			<m:MaterialLink text="Sign out"/>
		</m:MaterialDropDown>
		</m:html.Div>
	</a:MaterialSideProfile>
</ui:UiBinder> 