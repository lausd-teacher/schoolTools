<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:g="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	
	</ui:style>
	<b:Panel>
		<b:PanelHeader ui:field="hwAttToolbar">
		<b:ListBox></b:ListBox>
		<b:Navbar>
		<b:NavbarNav ui:field="attendanceTool">
			<b:ImageAnchor></b:ImageAnchor>
			<b:Divider />
			<b:ImageAnchor></b:ImageAnchor>
		</b:NavbarNav>
		</b:Navbar>
		</b:PanelHeader>
		<b:PanelBody ui:field="mainStudentPanel">
			<g:HTMLPanel>
			<div class="seatingChartStudentMore pull-right">
			<i class="fa fa-elipsis-v" />
			</div>	
			<section class="pull-left">
				<b:ImageAnchor ui:field="studentImg" />
				<br/>
				<span ui:field="studentFistName" />&nbsp;&nbsp;<span ui:field="studentLastName" />
				<br/>
				<hr/>
				<span ui:field="jobsList"></span>
			</section>
			</g:HTMLPanel>
		</b:PanelBody>
		<b:PanelFooter ui:field="behaviorToolbar">
			<b:Navbar ui:field="quickBehavior">
			<b:ImageAnchor ui:field="addNegativeBtn"></b:ImageAnchor>
			<g:HTMLPanel ui:field="behaviorGauge"></g:HTMLPanel>
			<b:ImageAnchor ui:field="addPositiveBtn"></b:ImageAnchor>
			</b:Navbar>
		</b:PanelFooter>
	</b:Panel>
</ui:UiBinder> 