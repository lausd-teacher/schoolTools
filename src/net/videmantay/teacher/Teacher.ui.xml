<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:g="urn:import:org.gwtbootstrap3.client.ui.gwt">
	<ui:style>
		.mainContainer{
		overflow-y:hidden;
	}
	</ui:style>
	<b:Container>
		<b:Panel>
			<b:PanelHeader>
				<b:Navbar ui:field="navbar">
					<b:NavbarBrand></b:NavbarBrand>
					<b:NavbarCollapse>
						<b:NavbarNav>
							<b:AnchorListItem ui:field="dashboardBtn">Dashboard</b:AnchorListItem>
							<b:AnchorListItem ui:field="rosterBtn">Roster</b:AnchorListItem>
							<b:AnchorListItem ui:field="gradesBtn">Grades</b:AnchorListItem>
							<b:AnchorListItem ui:field="lessonsBtn">Lessons</b:AnchorListItem>
							<b:AnchorListItem ui:field="calendarBtn">Calendars</b:AnchorListItem>
							<b:AnchorListItem ui:field="libraryBtn">Library</b:AnchorListItem>
							<b:AnchorListItem ui:field="vocabBtn">Vocabulary</b:AnchorListItem>
							<b:AnchorListItem ui:field="commonCoreBtn">Common core</b:AnchorListItem>
						</b:NavbarNav>
						<b:NavbarForm>
							<b:InputGroup>
								<b:InputGroupAddon icon="SEARCH"/>
								<b:Input type="SEARCH" />
							</b:InputGroup>
						</b:NavbarForm>
					</b:NavbarCollapse>
					<b:NavbarCollapseButton ui:field="collapseBtn" />
				</b:Navbar>
			</b:PanelHeader>
		</b:Panel>
		<b:PanelBody ui:field="mainPanel">
		<b:Row>
			<b:Column ui:field="asideCol" size="MD_3">
				<g:HTMLPanel>
					<div ui:field="asideMenu" class="well">
						<table><!--  -->
							<tr><td></td></tr>
						</table>
					</div>
				</g:HTMLPanel>
			</b:Column>
			<b:Column size="MD_12" ui:field="mainCol"></b:Column>
		</b:Row>
		</b:PanelBody>
		<b:PanelFooter ui:field="footerPanel"></b:PanelFooter>
	</b:Container>
</ui:UiBinder> 