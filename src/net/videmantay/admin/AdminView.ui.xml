<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:v="urn:import:net.videmantay.admin">
	<b:Container>
		<b:Panel>
			<b:PanelHeader>
				<b:PageHeader subText="Users">Admin Console</b:PageHeader>
				<b:Icon ui:field="userAddButton" type="USER"></b:Icon>
			</b:PanelHeader>
		<b:PanelBody ui:field="mainPanel">
		</b:PanelBody>
			</b:Panel>
			<b:Modal ui:field="modal">
				<b:ModalBody>
					<v:AppUserEditor ui:field="editor" />
				</b:ModalBody>
			</b:Modal>
			<b:Modal ui:field="deleteModal">
				<b:ModalBody>
					<b:Well><g:HTMLPanel><h3>Are you sure you want to delete the user?</h3></g:HTMLPanel></b:Well>
					<b:ButtonToolBar>
						<b:Button ui:field="deleteButton" type="DANGER">Delete</b:Button>
						<b:Button ui:field="cancelButton" type="INFO">Cancel</b:Button>
					</b:ButtonToolBar>
				</b:ModalBody>
			</b:Modal>
	</b:Container>	
</ui:UiBinder> 