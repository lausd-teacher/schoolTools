<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
	<b:Container>
	<b:Panel><b:PanelBody>
		<b:Form ui:field="userForm">
			<b:FormGroup>
				<b:FormLabel for="acctId" >Account ID:</b:FormLabel>
					<b:Input type="EMAIL" ui:field="acctId"/>
				</b:FormGroup>
				<b:FormGroup>
				<b:FormLabel for="firstName" >First Name:</b:FormLabel>
					<b:Input type="TEXT" ui:field="firstName"/>
				</b:FormGroup>
				<b:FormGroup>
				<b:FormLabel for="lastName" >Last Name:</b:FormLabel>
					<b:Input type="TEXT" ui:field="lastName"/>
				</b:FormGroup>
				<b:FormGroup>
				<b:FormLabel for="middleName" >Middle Name:</b:FormLabel>
					<b:Input type="TEXT" ui:field="middleName"/>
				</b:FormGroup>
				<b:FormGroup>
				<b:FormLabel for="title" >Title:</b:FormLabel>
				<b:ListBox ui:field="title">
				<b:item value="MR">Mr.</b:item>
				<b:item value="MS">Ms.</b:item>
				<b:item value="MRS">Mrs.</b:item>
				</b:ListBox>
				</b:FormGroup>
				<b:FormGroup>
				<b:FormLabel for="mainDriveFolderId" >Pic:</b:FormLabel>
				<b:InputGroup>
					<b:Input type="FILE" enabled="false" ui:field="mainDriveFolderId"/>
					<b:InputGroupAddon icon="FOLDER" />
					</b:InputGroup>
				</b:FormGroup>	
				<gwt:Hidden name="user" ui:field="userInput"></gwt:Hidden>	
		</b:Form>
		</b:PanelBody>
		<b:PanelFooter>
		<b:ButtonGroup>
			<b:Button ui:field="submit" type="INFO" >Submit</b:Button>
			<b:Button ui:field="cancelSave" type="DANGER">Cancel</b:Button>
		</b:ButtonGroup>
		</b:PanelFooter></b:Panel>
		</b:Container>
</ui:UiBinder> 