<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<m:MaterialCard ui:field="card" backgroundColor="green">
		<m:MaterialCardTitle padding="5.0" textAlign="LEFT" ui:field="cardTitle" />
		<m:MaterialCardContent ui:field="cardContent" backgroundColor="white" textColor="green darken-3">
			<m:MaterialLabel ui:field="cardDescript"></m:MaterialLabel>
		</m:MaterialCardContent>
		<m:MaterialCardAction  height="3em" ui:field="cardAction">
		<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialIcon textColor="green darken-4" ui:field="folderIcon" iconType="FOLDER"/>
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialIcon textColor="green darken-4" ui:field="eventIcon" iconType="EVENT"/>
			</m:MaterialColumn>
			<m:MaterialIcon textColor="green darken-4" ui:field="infoIcon" iconType="LIVE_HELP"/>
		</m:MaterialRow>
		</m:MaterialCardAction>
	</m:MaterialCard>
</ui:UiBinder> 