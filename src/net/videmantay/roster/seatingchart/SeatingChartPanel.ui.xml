<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
		.absolute{position:absolute}
	</ui:style>
	<g:HTMLPanel styleName="seatingChartPanel">
		<header>
			<m:MaterialPanel id="furnitureToolBar">
				<m:MaterialRow ui:field="furnitureIconList">
					
				</m:MaterialRow>
			</m:MaterialPanel>
		</header>
		<div class="mainWrapper">
		<m:MaterialSideNav ui:field="studentList">
		<m:MaterialLink ui:field="studentToolPanel" />
		</m:MaterialSideNav>
		<m:MaterialContainer ui:field="floorPlan" addStyleName="{style.absolute}">

		</m:MaterialContainer>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 