<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
		.absolute{position:absolute}
	</ui:style>
	<g:HTMLPanel styleName="seatingChartPanel">
		<header>
			<m:html.Nav id="furnitureToolBar"></m:html.Nav>
			<m:html.Nav id="studentToolBar"></m:html.Nav>
		</header>
		<m:MaterialSideNav></m:MaterialSideNav>
		<m:MaterialContainer ui:field="floorPlan" addStyleName="{style.absolute}">

		</m:MaterialContainer>
	</g:HTMLPanel>
</ui:UiBinder> 