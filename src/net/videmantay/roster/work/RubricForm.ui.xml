<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	</ui:style>
	<g:FormPanel>
	<g:HTMLPanel>
		<m:MaterialTextBox placeholder="Name Optional" />
		<m:MaterialLabel text="* Name the rubric to save for later use."></m:MaterialLabel>
		<br/>
		<m:html.Div ui:field="itemList">
		<m:MaterialRow ui:field="headerRow" grid="s3">
			<m:MaterialColumn ui:field="descriptCol">
				<m:MaterialLabel text="Criteria" />
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="bestCol">
				<m:MaterialLabel text="4" />
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="goodCol">
				<m:MaterialLabel text="3" />
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="badCol">
				<m:MaterialLabel text="2" />
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="worstCol">
				<m:MaterialLabel text="1" />
			</m:MaterialColumn>
		</m:MaterialRow>
		</m:html.Div>
	</g:HTMLPanel>
	</g:FormPanel>
</ui:UiBinder> 