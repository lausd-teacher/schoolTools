<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
		.absolutePos{
			position:absolute;
			top:0px;
			left:0px;
	}
	</ui:style>
	<m:MaterialPanel><!-- -->
		<g:HTMLPanel>
			<div style="width:64px; height:64px;position:relative;">
				<m:MaterialImage ui:field="studentImg" width="100%"
					height="100%" />
					<div ui:field="iconPanel" 
					style="width:100%;height:100%;z-index:3; opacity:0.5;border-radius:100%; border:1px groove Chartruse" ></div>
			</div>
			<div>
				<m:MaterialLabel ui:field="firstName" />
				<m:MaterialLabel ui:field="lastName" />
			</div>
			
		</g:HTMLPanel>
	</m:MaterialPanel>
</ui:UiBinder> 