<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:e="urn:import:gwt.material.design.addins.client.richeditor">
	<g:HTMLPanel>
		<m:MaterialRow> 
			<m:MaterialImage ui:field="studentImg"  />
			<m:MaterialLabel ui:field="firstName" />
			<m:MaterialLabel ui:field="lastName"/>
			<m:MaterialTextBox ui:field="points"/>
			<m:MaterialTextBox ui:field="percentage"/>
			<:MaterialTextBox ui:field="letterGrade"/>
			<m:MaterialCheckBox ui:field="turnedIn"/>
			<m:MaterialLabel ui:field="dueDate" />
			<m:MaterialDatePicker ui:field="turnedInDate" />
		</m:MaterialRow>
		<m:MaterialRow> 
		<m:MaterialIcon ui:field="showMessageIcon" iconType="MORE_HORZ" />
		<e:MaterialRichEditor ui:field="comment" placeholder="Teacher Comment" />
		</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 