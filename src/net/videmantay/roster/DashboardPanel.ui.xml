<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<g:HTMLPanel height="100%" width="100%">
		<div id="roster-dashboard">
		<div class="amber darken-1" style="height: 2em;">
		<img class="scTeacherImg left circle responsive-img" src="https://cdn3-7.cdn.schoology.com/system/files/imagecache/profile_reg/pictures/picture-5eb006aea3cf8fc893caf08282c4fa70_5919ebb9ba1d1.png" 
		style="width: 65px; height: 65px;margin-left: 20px; margin-top:5px; border:1px solid brown; border-radius:100%" /><span class="sectionTitle center-align white-text material-label" style="font-size: 1em; font-weight: bold; margin-left:30% ;">2307 Elementary Gradebook Training Section 1</span><button type="button" class="helpBtn amber accent-1 center-align btn-floating" style="cursor: pointer; font-weight: bold; margin-left: 5px; height: 2.5em;"><i class="left white-text large material-icons" style="cursor: pointer;">help_outline</i></button></div>
		<m:MaterialRow ui:field="toolbar" height="3em"
				padding="2.5" backgroundColor="AMBER_DARKEN_2">
				<m:MaterialColumn grid="s3" float="RIGHT">
				<m:MaterialIcon ui:field="editBtn" iconType="SETTINGS" float="RIGHT" />
				<m:MaterialAnchorButton ui:field="routineBtn"
					addStyleNames="classTimeBtn truncate"  text="Your Seats"
					  textAlign="CENTER"
					iconType="ARROW_DROP_DOWN" iconSize="MEDIUM" iconPosition="RIGHT"
					activates="routinelist" />
				<m:MaterialDropDown activator="routinelist"
					ui:field="routineDrop">
					<m:MaterialLink addStyleNames="manageClassTimeLink"
						text="Manage..." />
				</m:MaterialDropDown>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s7">
					<m:MaterialColumn grid="s1" marginLeft="15">
						<m:MaterialLabel text="Check:"></m:MaterialLabel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2">
							<g:HTMLPanel>
								<m:MaterialTooltip text="Roll">
									<m:MaterialIcon ui:field="rollIcon"
										iconType="ASSIGNMENT_IND" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2">
							<g:HTMLPanel>
								<m:MaterialTooltip text="Homework">
									<m:MaterialIcon ui:field="hwIcon"
										iconType="ASSIGNMENT" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					
					<!-- This starts selects tools -->
					<m:MaterialColumn grid="s1" marginLeft="15">
						<m:MaterialLabel text="Select:"></m:MaterialLabel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel marginLeft="10" padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Multiple">
									<m:MaterialIcon ui:field="multipleIcon"
										iconType="GROUP_ADD" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Random">
									<m:MaterialIcon ui:field="randomIcon"
										iconType="SHUFFLE" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="All">
									<m:MaterialIcon ui:field="selectAllIcon"
										iconType="SELECT_ALL" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<!-- this start the view tools -->
					<m:MaterialColumn grid="s1" marginLeft="15">
						<m:MaterialLabel text="View:"></m:MaterialLabel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Home">
									<m:MaterialIcon ui:field="homeIcon"
										iconType="HOME" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Groups">
									<m:MaterialIcon ui:field="groupsIcon"
										iconType="GROUP_WORK" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Stations">
									<m:MaterialIcon ui:field="stationsIcon"
										iconType="WIDGETS" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow ui:field="doneToolbar" backgroundColor="RED_DARKEN_1">
					<m:MaterialColumn grid="m3" marginLeft="25">
						<m:MaterialAnchorButton ui:field="clearAllBtn"
							text="Clear All" iconType="DELETE"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="m3">
						<m:MaterialAnchorButton ui:field="doneBtn" text="Done"
							iconType="DONE" backgroundColor="GREEN"/>
					</m:MaterialColumn>
					<m:MaterialColumn grid="m3">
						<m:MaterialAnchorButton
							ui:field="doneBarCancelBtn" text="CANCEL" iconType="CLEAR" backgroundColor="RED"></m:MaterialAnchorButton>
					</m:MaterialColumn>
			</m:MaterialRow>
			<g:HTMLPanel ui:field="mainPanel" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 