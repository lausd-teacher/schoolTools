<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel height="100%" width="100%">
		<div id="roster-dashboard-tab">
			<m:MaterialRow ui:field="toolbar" height="3em"
				padding="5" marginBottom="0" centerOn="CENTER_ON_SMALL"
				backgroundColor="AMBER_DARKEN_2">
				<m:MaterialIcon iconType="SETTINGS" float="RIGHT" />
				<m:MaterialAnchorButton ui:field="routineBtn"
					addStyleNames="classTimeBtn" float="RIGHT" text="Class Time"
					fontSize="1.10em" marginRight="5.0" textAlign="CENTER"
					iconType="ARROW_DROP_DOWN" iconSize="MEDIUM" iconPosition="RIGHT"
					activates="routinelist" />
				<m:MaterialDropDown activator="routinelist"
					ui:field="routineDrop">
					<m:MaterialLink addStyleNames="manageClassTimeLink"
						text="Manage..." />
				</m:MaterialDropDown>
				<m:MaterialColumn grid="s12 m8 l6">
					<m:MaterialColumn grid="s1">
						<m:MaterialLabel text="Check:"></m:MaterialLabel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2">
							<g:HTMLPanel>
								<m:MaterialTooltip text="Roll">
									<m:MaterialIcon ui:field="rollIcon"
										iconType="ASSIGNMENT_IND" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2">
							<g:HTMLPanel>
								<m:MaterialTooltip text="Homework">
									<m:MaterialIcon ui:field="hwIcon"
										iconType="ASSIGNMENT" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Groups">
									<m:MaterialIcon ui:field="groupsIcon"
										iconType="GROUP_WORK" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Stations">
									<m:MaterialIcon ui:field="stationsIcon"
										iconType="WIDGETS" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<!-- This starts selects tools -->
					<m:MaterialColumn grid="s1" offset="s2">
						<m:MaterialLabel text="Select:"></m:MaterialLabel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel marginLeft="10" padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Multiple">
									<m:MaterialIcon ui:field="multipleIcon"
										iconType="GROUP_ADD" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="Random">
									<m:MaterialIcon ui:field="randomIcon"
										iconType="SHUFFLE" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s1">
						<m:MaterialPanel padding="2"><!-- -->
							<g:HTMLPanel>
								<m:MaterialTooltip text="All">
									<m:MaterialIcon ui:field="selectAllIcon"
										iconType="SELECT_ALL" iconSize="SMALL" />
								</m:MaterialTooltip>
							</g:HTMLPanel>
						</m:MaterialPanel>
					</m:MaterialColumn>
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow ui:field="doneToolbar">
				<m:MaterialColumn hideOn="HIDE_ON_SMALL"
					ui:field="fullDoneToolbar" grid="s12 m12 l12">
					<m:MaterialColumn grid="m3">
						<m:MaterialAnchorButton ui:field="doneBtn"
							text="DONE" iconType="DONE"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="m3">
						<m:MaterialButton ui:field="undoBtn" text="Undo"
							iconType="UNDO" />
					</m:MaterialColumn>
					<m:MaterialColumn grid="m3">
						<m:MaterialAnchorButton
							ui:field="doneBarCancelBtn" text="CANCEL" iconType="CLEAR"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="m3">
						<m:MaterialButton type="FLAT" text="Mangage"
							iconType="MORE_VERT" iconPosition="RIGHT" />
					</m:MaterialColumn>
				</m:MaterialColumn>
				<m:MaterialColumn ui:field="smDoneToolbar"
					hideOn="HIDE_ON_MED_UP">
					<m:MaterialColumn grid="s3">
						<m:MaterialButton ui:field="smDoneBtn"
							iconType="DONE"></m:MaterialButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s3">
						<m:MaterialButton ui:field="smUndoBtn"
							iconType="UNDO" />
					</m:MaterialColumn>
					<m:MaterialColumn grid="s3">
						<m:MaterialButton ui:field="smDoneBarCancelBtn"
							iconType="CLEAR"></m:MaterialButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s3">
						<m:MaterialButton type="FLAT"
							iconType="MORE_VERT" />
					</m:MaterialColumn>
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialAnchorButton ui:field="routineBar"
				centerOn="CENTER_ON_SMALL" grid="s12" width="100%"
				hideOn="HIDE_ON_MED_UP" addStyleNames="classTimeBar"
				text="Class Time" fontSize="1.10em" textAlign="CENTER"
				iconType="ARROW_DROP_DOWN" iconSize="MEDIUM" iconPosition="RIGHT"
				activates="routinedrop"></m:MaterialAnchorButton>
			<m:MaterialDropDown activator="routinedrop"
				ui:field="routineDrop2">
				<m:MaterialLink addStyleNames="manageClassTimeLink"
					text="Manage..." />
			</m:MaterialDropDown>
			<g:HTMLPanel ui:field="mainPanel" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 