<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel ui:field="rosterStudentPanel" styleName="rosterStudent">
		<table><!--  -->
			<tr><!-- Student Image Row -->
				<td colspan="2">
					<div class="counterRotate">
						<m:MaterialImage ui:field="studentImg" addStyleName="studentImg" />
					</div>
				</td>
			</tr>
			<tr><!-- Body of studentPane including name panel and behavior tool bar -->
				<td>
				<div class="counterRotate"><!-- wrapped around a div for rotatable -->
				<table>
				<tr>
				<td rowspan="2">
					<m:html.Div ui:field="studentName" class="studnetName">
						<m:html.Span ui:field="firstName" addStyleName="first"/><br/>
						<m:html.Span ui:field="lastName" addStyleName="last" />
					</m:html.Div>
				</td>
				<td rowspan="1">
					<m:html.Div ui:field="studentCheckBox" addStyleName="studentCheckbox"></m:html.Div>
				</td>
			</tr>
			<tr class="inicidentBar">
				<td><m:MaterialImage ui:field="negIncident" addStyleName="negIncident"/></td>
				<td><div class="middleBarIncident"></div></td>
				<td><m:MaterialImage ui:field="posIncident" addStyleName="posIncident"/></td>
			</tr>
			</table>
			</div>
			</td>
		</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 