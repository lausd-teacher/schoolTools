<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html"
	xmlns:v="urn:import:net.videmantay.shared"
	xmlns:vr="urn:import:net.videmantay.roster">
	<g:HTMLPanel styleName="classroom" ui:field="classroom">
			<m:MaterialNavBar activates="sideNav" backgroundColor="green">
			
			<m:MaterialNavBrand  ui:field="rosterTitle" textAlign="CENTER"/>
			</m:MaterialNavBar>

			<m:MaterialSideNav type="CLIP" m:id="sideNav" ui:field="sideNav">
				<v:UserProfilePanel></v:UserProfilePanel>
				<m:MaterialLink ui:field="dashboardLink" iconType="DASHBOARD" text="Dashboard" textColor="grey darken-4"/>
				<m:MaterialLink ui:field="studentLink" addStyleNames="studentLink classLink" text="Students" textColor="green" iconType="CLASS" />
				<m:MaterialLink ui:field="assignmentLink" text="Assignments" textColor="orange" iconType="EVENT" />
				<m:MaterialLink ui:field="groupLink" text="Groups" textColor="light-green darken-4"
					iconType="LIBRARY_BOOKS"/>
				<m:MaterialLink ui:field="lessonPlanLink" text="Lesson Plans" textColor="amber darken-4"
					iconType="CONTENT_PASTE" />
				<m:MaterialLink ui:field="incidentLink" text="Incidents" textColor="green"
					iconType="SETTINGS" />
				<m:MaterialLink ui:field="goalLink" text="Goals" textColor="green" iconType="CLASS" />
				<m:MaterialLink ui:field="jobLink" text="Jobs" textColor="green" iconType="CLASS" />
				<m:MaterialLink ui:field="seatingChartLink" text="Seatingcharts" textColor="green" iconType="EVENT_SEAT" />
				<m:MaterialLink></m:MaterialLink>
			</m:MaterialSideNav>
		<m:MaterialContainer  ui:field="mainPanel" width="100%" height="100%" >
		</m:MaterialContainer>
	</g:HTMLPanel>
</ui:UiBinder> 