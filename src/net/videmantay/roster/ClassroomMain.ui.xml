<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html"
	xmlns:v="urn:import:net.videmantay.shared"
	xmlns:vr="urn:import:net.videmantay.roster">
	<g:HTMLPanel styleName="classroom" ui:field="classroom">
			<m:MaterialNavBar activates="sideNav">
			
			<m:MaterialNavBrand  ui:field="rosterTitle" textAlign="CENTER"/>
			</m:MaterialNavBar>

			<m:MaterialSideNavDrawer  m:id="sideNav" ui:field="sideNav">
				<v:UserProfilePanel></v:UserProfilePanel>
				<m:MaterialLink ui:field="dashboardLink" iconType="DASHBOARD" text="Dashboard" />
				<m:MaterialLink ui:field="studentLink" addStyleNames="studentLink classLink" text="Students" iconType="SCHOOL" />
				<m:MaterialLink ui:field="routineLink" text="Routines" iconType="SCHEDULE" />
				<m:MaterialLink ui:field="assignmentLink" text="Assignments" iconType="ASSIGNMENT" />
				<m:MaterialLink ui:field="lessonPlanLink" text="Lesson Plans" iconType="LIBRARY_BOOKS" />
				<m:MaterialLink ui:field="incidentLink" text="Incidents" iconType="FEEDBACK" />
				<m:MaterialLink ui:field="goalLink" text="Goals" iconType="TRENDING_UP" />
				<m:MaterialLink ui:field="jobLink" text="Jobs" iconType="BUSINESS_CENTER" />
				<m:MaterialLink></m:MaterialLink>
			</m:MaterialSideNavDrawer>
		<m:MaterialContainer  ui:field="mainPanel" width="100%" height="100%" >
		</m:MaterialContainer>
	</g:HTMLPanel>
</ui:UiBinder> 